<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.musicismylife.domain.post.PostMapper">

    <!--게시글 목록 조회 -->
    <select id="getPostList" resultType="com.musicismylife.domain.post.PostDTO">
      
    </select>

    <!-- 회원 상세 조회 -->
    <select id="getPostById" parameterType="int" resultType="com.musicismylife.domain.post.PostDTO">
        
    </select>

    <!-- 회원 추가 -->
    <insert id="insertPost" parameterType="com.musicismylife.domain.post.PostDTO">
       
    </insert>

    <!-- 회원 수정 -->
    <update id="updatePost" parameterType="com.musicismylife.domain.post.PostDTO">
       
    </update>

    <!-- 회원 삭제 -->
    <delete id="deletePost" parameterType="int">
    
    </delete>

</mapper>